<launch>
 
	<!-- Lidar odometry param -->
	<rosparam file="$(find lvi_sam)/config/kitti/2011_09_30/KITTI_lidar.yaml" command="load" />
	<!-- VINS config -->
	<param name="vins_config_file" type="string" value="$(find lvi_sam)/config/kitti/2011_09_30/KITTI_camera.yaml" />


    <!--- Robot State TF -->
    <include file="$(find lvi_sam)/launch/include/module_robot_state_publisher.launch" />
    <!--- Run Rviz-->
    <include file="$(find lvi_sam)/launch/include/module_rviz.launch" />
    <!--- SAM -->
    <include file="$(find lvi_sam)/launch/include/module_sam.launch" />

    <!--- run python node inside depthmap_densifier -->
    <!-- <node pkg="depthmap_densifier" type="completer.py" name="completer.py" output="screen" /> -->
</launch>
